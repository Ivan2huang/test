{
  "name": "distribution-employee-frontend",
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "dev": "source ./.env.exports && cross-env NODE_ENV=development node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "next build src && webpack",
    "create-placeholder": "node translations/placeholder.js",
    "translate": "node translations/convert.js",
    "sentry-release-init": "sentry-cli releases new ${COMMIT_SHA}",
    "sentry-release-files": "sentry-cli releases files ${COMMIT_SHA} upload-sourcemaps dist/ --rewrite --validate",
    "sentry-final-release": "sentry-cli releases finalize ${COMMIT_SHA} && sentry-cli releases files ${COMMIT_SHA} list",
    "sourcemap-cleanup": "find dist/ -name \"*.map\" -type f -delete",
    "test": "jest -u",
    "lint": "eslint src --fix"
  },
  "lint-staged": {
    "*.{js,jsx,css,html,md,json,spec.jsx,spec.js}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
    }
  },
